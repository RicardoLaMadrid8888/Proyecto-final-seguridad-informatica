   "source": [
    "def get_soup(url):\n",
    "    content = requests.get(url).content\n",
    "    return BeautifulSoup(content, 'lxml') "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 40,
   "id": "69405f73",
   "metadata": {},
   "outputs": [],
   "source": [
    "def e_articulos(url=start_url):\n",
    "    soup = get_soup(url)\n",
    "    p_tags = soup.findAll('article')\n",
    "    a_tags = []\n",
    "    for p_tag in p_tags:\n",
    "        a_tags.extend(p_tag.select('h2 a'))\n",
    "        \n",
    "    a_tags = [a_tag for a_tag in a_tags if 'href' in a_tag.attrs]\n",
    "    links = [a_tag.get('href') for a_tag in a_tags]\n",
    "    return links"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 41,
   "id": "aed80bf7",
   "metadata": {},
   "outputs": [],
   "source": [
    "def e_noticias(url):\n",
    "    soup = get_soup(url)\n",
    "    titulo = soup.find('h1').text\n",
    "    articulos = soup.findAll('p')\n",
    "    return titulo,articulos"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 42,
   "id": "64389fbf",
   "metadata": {},
   "outputs": [],
   "source": [
    "if __name__=='__main__':\n",
    "    ext_links = e_articulos(start_url)\n",
    "    \n",
    "    noticias=[]\n",
    "    \n",
    "    with open('noticias_mundiall.txt','w',encoding='utf-8-sig')as f:\n",
    "        for noti in ext_links:\n",
    "            titulo,contenido=e_noticias(noti)\n",
    "            f.write(str(contenido)+'\\n')"
   ]
